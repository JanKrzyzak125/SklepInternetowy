<Window x:Class="SklepInternetowy.Authentication"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SklepInternetowy"
        mc:Ignorable="d"
        Title="Authentication" Height="450" Width="800">
    <Grid>
        <Button x:Name="LoginButton" Content="Zalogowac" HorizontalAlignment="Left" Height="53" Margin="167,295,0,0" VerticalAlignment="Top" Width="146" Click="LoginButton_Click"/>
        <Button x:Name="Registration" Content="Rejestracja" HorizontalAlignment="Left" Height="53" Margin="400,295,0,0" VerticalAlignment="Top" Width="167" Click="Registration_Open"/>
        <CheckBox Content="Zapamietaj" HorizontalAlignment="Left" Height="16" Margin="218,257,0,0" VerticalAlignment="Top" Width="292"/>
        <TextBox x:Name="NickText" HorizontalAlignment="Left" Height="41" Margin="240,132,0,0" Text="Nick" TextWrapping="Wrap" VerticalAlignment="Top" Width="293"/>
        <PasswordBox x:Name="PasswordBox" HorizontalAlignment="Left" Height="49" Margin="240,194,0,0" VerticalAlignment="Top" Width="293"/>

    </Grid>
</Window>
