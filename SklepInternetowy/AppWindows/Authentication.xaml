<Window x:Class="SklepInternetowy.Authentication"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SklepInternetowy"
        mc:Ignorable="d"
        Title="Logowanie" Height="300" Width="400">
    <Grid>
        <Button x:Name="LoginButton" Content="Zalogowac" HorizontalAlignment="Left" Height="53" Margin="10,195,0,0" VerticalAlignment="Top" Width="143" Click="LoginButton_Click" Cursor="Hand"/>
        <Button x:Name="Registration" Content="Rejestracja" HorizontalAlignment="Left" Height="53" Margin="168,195,0,0" VerticalAlignment="Top" Width="148" Click="Registration_Open"/>
        <CheckBox Unchecked="Password_Uncheck" Checked="Password_Check" x:Name="CheckBoxPassword" Content="Zapomniałem Hasła" HorizontalAlignment="Left" Height="24" Margin="22,153,0,0" VerticalAlignment="Top" Width="293"/>
        <TextBox x:Name="NickText" HorizontalAlignment="Left" Height="41" Margin="22,24,0,0" Text="Nick" TextWrapping="Wrap" VerticalAlignment="Top" Width="293"/>
        <PasswordBox x:Name="PasswordBox" HorizontalAlignment="Left" Height="40" Margin="22,89,0,0" VerticalAlignment="Top" Width="293"/>

    </Grid>
</Window>
