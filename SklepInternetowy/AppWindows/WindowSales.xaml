<Window x:Class="SklepInternetowy.AppWindows.WindowSales"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SklepInternetowy.AppWindows"
        mc:Ignorable="d"
        Title="WindowSales" 
        Height="450" Width="600"
        ResizeMode="CanMinimize"
        >
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="73*"/>
            <ColumnDefinition Width="73*"/>
            <ColumnDefinition Width="73*"/>
            <ColumnDefinition Width="73*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="25*"/>
            <RowDefinition Height="25*"/>
            <RowDefinition Height="25*"/>
            <RowDefinition Height="25*"/>
            <RowDefinition Height="25*"/>
            <RowDefinition Height="25*"/>

        </Grid.RowDefinitions>

        <Label Content="Ilość wystawiona*" 
               HorizontalAlignment="Center" VerticalAlignment="Center" 
               Height="33" Width="130" 
               Grid.Row="0"/>
        <Label Content="Data Startu" 
               HorizontalAlignment="Center" VerticalAlignment="Center" 
               Height="33" Width="130" 
               Grid.Row="1"/>
        <Label Content="Data zakończenia" 
               HorizontalAlignment="Center" VerticalAlignment="Center" 
               Height="33" Width="130" 
               Grid.Row="2"/>
        <Label Content="Dni zwrotu" 
               HorizontalAlignment="Center" VerticalAlignment="Center" 
               Height="33" Width="130" 
               Grid.Row="3"/>
        <Label Content="Rodzaj dostawy" 
               HorizontalAlignment="Center" VerticalAlignment="Center" 
               Height="33" Width="130" 
               Grid.Row="4"/>
        <Label Content="Zobacz produkt" 
               HorizontalAlignment="Center" VerticalAlignment="Center" 
               Height="33" Width="130" 
               Grid.Row="5"/>
        <Label Content="Liczba wizytacji" 
               HorizontalAlignment="Center" VerticalAlignment="Center" 
               Height="33" Width="130" 
               Grid.Column="2"/>
        <Label Content="Data wyłączenia" 
               HorizontalAlignment="Center" VerticalAlignment="Center" 
               Height="33" Width="130" 
               Grid.Column="2" Grid.Row="1"/>
        <Label Content="??" 
               HorizontalAlignment="Center" VerticalAlignment="Center" 
               Height="33" Width="130" 
               Grid.Column="2" Grid.Row="2"/>
        <Label Content="???" 
               HorizontalAlignment="Center" VerticalAlignment="Center" 
               Height="33" Width="130" 
               Grid.Column="2" Grid.Row="3"/>
        <Label Content="Status" 
               HorizontalAlignment="Center" VerticalAlignment="Center" 
               Height="33" Width="130" 
               Grid.Column="2" Grid.Row="4"/>
        <Label Content="Zatwierdź" 
               HorizontalAlignment="Center" VerticalAlignment="Center" 
               Height="33" Width="130" 
               Grid.Column="2" Grid.Row="5"/>
       


        <Button Content="Button" Grid.Column="3" HorizontalAlignment="Center" Grid.Row="5" VerticalAlignment="Center" Height="33" Width="130" Margin="0,4,0,0"/>
        <Button Content="Produkt"
                HorizontalAlignment="Center" VerticalAlignment="Center"
                Height="35" Width="130" 
                Grid.Column="1" Grid.Row="5"
                ToolTip="Włącz podgląd produktu" Click="ButtonProduct_Click"
                />
        <DatePicker Grid.Column="1" HorizontalAlignment="Center" Height="33" Grid.Row="1" Width="130"/>




    </Grid>
</Window>
